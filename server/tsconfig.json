{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2020",
    "jsx": "react-jsx",
    // "allowImportingTsExtensions": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "alwaysStrict": true
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}
